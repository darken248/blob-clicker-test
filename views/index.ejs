<!DOCTYPE html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css">
    <script src="/version"></script>
    <script>
        window.onload = function(){
            document.getElementById('version').innerText = version + ' is out'
            alert('Sadly Blobclicker will be discounted and will be shutdown at the end of the month')
        }
        function info(){
            location.href = "/info"
        }
        function game(){
            location.href = "/game"
        }
        function creators(){
            location.href = "/creators"
        }
        function patch(){
            location.href = "/patch"
        }
        function lb(){
            location.href = "/leaderboard"
        }
    </script>
    <title>Blob Clicker</title>
    
    <link rel="shortcut icon" type="image/x-icon" href="img/blob/yellow blob.gif"/>
</head>
<body>
    <button class="menucreators" onclick="creators()"><img src="img/about the creators.png" height="90"></button>
    <button class="menuinfo" onclick="info()"><img src="img/info.png" height="80"></button>
    <button class="menupatch" onclick="patch()"><img src="img/patch notes2.png" height="90"></button>
    <button class="menulb" onclick="lb()"><img src="img/leaderboard.png" height="90"></button>
    <br><br><br><br>
    <img class="homescreen" onclick="game()" src="img/homescreen.png">
    <h1 class="versions" id="version">V 1.0.0 is out</h1>
</body>
</html>
